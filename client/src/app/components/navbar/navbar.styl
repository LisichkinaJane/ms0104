.navbar-wrapper
     background: -webkit-linear-gradient(top, #2725C7 0%, #0D7DC2 100%)
    
.navbar
    text-align left
    height 70px
    padding 10px
    
.navbar li
    display inline-block
    list-style none
    width 70px
    font-size 18px
    font-weight bold
    padding 10px
    margin-right 20px
    text-transform uppercase
    
.navbar li a 
    text-decoration none
    color #d9d9d9
    
.navbar li a:hover
    text-decoration none
    color #ffffff
    
.logo-wrap
    font-size 30px
    padding 20px
    font-weight 700
    height 150px
    background #0D7DC2
    margin-bottom 20px
    
.logo-wrap a 
    text-decoration none
    color #d9d9d9
    
.logo-wrap a:hover
    text-decoration none
    color #ffffff
    
    
